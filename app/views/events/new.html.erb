<%= form_for @event do |form| %>
  <div>
    <%= form.label :title %>
    <%= form.text_field :title, autofocus: true %>
  </div>

  <div class="form-group">
    <%= form.label :description%>
    <%= form.text_field :description %>
  </div>

  <div class="form-group">
    <%= form.label :date %>
    <%= form.datetime_local_field :date %>
  </div>

  <button type="submit">
    Add event
  </button>
<% end %>
